<style type="text/css">
  .user-profile-img {
  width: 100px;
  height: 100px;
  border-radius: 500px;
  overflow: hidden;
}
.user-profile-img img {
  min-height: 100px;
  width: 100%;
  border-radius: 0 !important;
}
  .matchuserpic {
  width: 100px;
  height: 100px;
  /*border-radius: 500px;*/
  overflow: hidden;
}
.matchuserpic img {
  min-height: 100px;
  width: 100%;
  border-radius: 0 !important;
}
</style>
  <main class="app-content">
    <div class="row">
      <div class="col-md-12">
        <div class="tile">
          <section class="invoice">
            <div class="row mb-4">
              <div class="col-6">
                <h2 class="page-header"><i class="fa fa-user"></i> View User</h2>
              </div>
            </div>
            <div class="row invoice-info">
              <div class="col-4"><h5><b>Name:</b>  &nbsp;&nbsp;&nbsp;&nbsp;{{data.name}}</h5><b>Country:&nbsp;&nbsp;&nbsp;</b> {{data.country}}<br><b>Mobile:&nbsp;&nbsp;&nbsp;&nbsp;</b> {{data.mobile}}<br><b>Gender:&nbsp;&nbsp;&nbsp;&nbsp;</b> {{data.gender}}<br><b>Seeking:&nbsp;&nbsp;&nbsp;</b>{{data.seeking}}<br><b>Age:&nbsp;&nbsp;&nbsp;</b> {{data.age}} <br><h5>Registration Date: {{data.date}}</h5></div>
              <div class="col-4">
              <div class="user-profile-img">
                 <!-- <img class="app-sidebar__user-avatar" src="{{#if image}}/images/{{image}}{{else}}/images/dummy.png{{/if}}" alt="User Image"> -->
                 <img class="app-sidebar__user-avatar" src="{{#if image}}http://18.216.167.102:3001/{{image}}{{else}}/images/dummy.png{{/if}}" alt="User Image">
              </div>
            </div>
            </div>
            <br>
            <div class="row">
              <div class="col-12 table-responsive">
             <h4>Matches</h4>
                <table class="table table-striped">
                  <thead>
                    <tr>
                      <th>Name</th>
                      <th>Country</th>
                      <th>Gender</th>
                      <th>Mobile</th>
                      <th>Image</th>
                    </tr>
                  </thead>
                  <tbody>
                    {{#each match}}
                      <tr>
                      <td>{{this.liked_user_id.name}}</td>
                      <td>{{this.liked_user_id.country}}</td>
                      <td>{{this.liked_user_id.gender}}</td>
                      <td>{{this.liked_user_id.mobile}}</td>
                      <td>{{{this.liked_user_id.images}}}</td>
                    </tr>
                    {{/each}}
                  </tbody>
                </table>
              </div>
            </div>
<!--             <div class="row d-print-none mt-2">
              <div class="col-12 text-right"><a class="btn btn-primary" href="javascript:window.print();" target="_blank"><i class="fa fa-print"></i> Print</a></div>
            </div> -->
          </section>
        </div>
      </div>
    </div>
  </main>